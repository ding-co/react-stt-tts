{
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 100,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "arrowParens": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/providers/(.*)$",
    "^@/engines/(.*)$",
    "^@/services/(.*)$",
    "^@/hooks/(.*)$",
    "^@/constants/(.*)$",
    "^@/types/(.*)$",
    "^[./]"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
